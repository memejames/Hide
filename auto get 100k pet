local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/memejames/Nothing-project1/main/Suku')))()

getgenv().toilet = false;
getgenv().oilet = false;

local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local char = lp.Character
local hrp = char.HumanoidRootPart

local Window = OrionLib:MakeWindow({Name = "Hade's RNGâ”‚F Hub DLC", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest", IntroText = "F Hub DLC", IntroIcon = "rbxassetid://14500306259", IntroEnabled = true, Icon = "rbxassetid://14500306259"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://7733955511",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Map",
	Icon = "rbxassetid://7733960981",
	PremiumOnly = false
})

-- Toggles

Tab:AddToggle({
	Name = "Auto Collect Pray",
	Default = false,
	Callback = function(bool)
		getgenv().toilet = bool
		if bool == true then
		toilet()
	end
end
})

Tab:AddToggle({
	Name = "Auto Collect Gwa Gwa",
	Default = false,
	Callback = function(bool)
		getgenv().oilet = bool
		if bool == true then
		oilet()
	end
end
})

-- Buttons

Tab2:AddButton({
	Name = "No JumpCooldown",
	Callback = function()
	    game.Players.LocalPlayer.Character.JumpCD:Destroy()
end
})

Tab:AddButton({
	Name = "On Auto Roll",
	Callback = function()
	    local args = {
    [1] = "AutoRoll",
    [2] = true
}

game:GetService("ReplicatedStorage").Network.Rolling:InvokeServer(unpack(args))

OrionLib:MakeNotification({
	Name = "Rejoin Game!",
	Content = "Need to rejoin for this to work!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end    
})

Tab:AddButton({
	Name = "Off Auto Roll",
	Callback = function()
	    local args = {
    [1] = "AutoRoll",
    [2] = false
}

game:GetService("ReplicatedStorage").Network.Rolling:InvokeServer(unpack(args))

OrionLib:MakeNotification({
	Name = "Rejoin Game!",
	Content = "Need to rejoin for this to work!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end    
})

-- Useless

Tab2:AddButton({
	Name = "TP to Pray",
	Callback = function()
		local player = game.Players.LocalPlayer
		local workspace = game:GetService("Workspace")

		local foundPray = false

		for _, v in pairs(workspace:GetChildren()) do
			if v.Name == "Pray" then
				foundPray = true
				player.Character.HumanoidRootPart.CFrame = v.CFrame
				break
			end
		end

		if not foundPray then
			OrionLib:MakeNotification({
				Name = "Pray Fruit not found",
				Content = "Wait 30 seconds",
				Image = "rbxassetid://4483345998",
				Time = 5
			})
		end
	end
})

-- functions

function toilet()
    spawn(function()
        while getgenv().toilet do
            local toiletSplash = game.workspace:FindFirstChild("Pray")
                if toiletSplash then
                    hrp.CFrame =  toiletSplash.CFrame
                    fireproximityprompt(game:GetService("Workspace").Pray.Attachment.ProximityPrompt, 0)
                end
            wait(0.5)
         end
    end)
end

function oilet()
    spawn(function()
        while getgenv().oilet do
            local toiletSplash = game.workspace:FindFirstChild("GwaGwa")
                if toiletSplash then -- Brain no working
                    hrp.CFrame =  toiletSplash.CFrame
                    fireproximityprompt(game:GetService("Workspace").GwaGwa.Attachment.ProximityPrompt, 0)
                end
            wait(0.5)
         end
    end)
end


OrionLib:Init()

