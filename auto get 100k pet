local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/memejames/Nothing-project1/main/Suku')))()

local Window = OrionLib:MakeWindow({Name = "Hages RNGâ”‚F Hub DLC", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest", IntroText = "F Hub DLC", IntroIcon = "rbxassetid://14500306259", IntroEnabled = true, Icon = "rbxassetid://14500306259"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Map",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "On Auto Roll",
	Callback = function()
	    local args = {
    [1] = "AutoRoll",
    [2] = true
}

game:GetService("ReplicatedStorage").Network.Rolling:InvokeServer(unpack(args))

OrionLib:MakeNotification({
	Name = "Rejoin Game!",
	Content = "Need to rejoin for this to work!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end    
})

Tab:AddButton({
	Name = "Off Auto Roll",
	Callback = function()
	    local args = {
    [1] = "AutoRoll",
    [2] = false
}

game:GetService("ReplicatedStorage").Network.Rolling:InvokeServer(unpack(args))

OrionLib:MakeNotification({
	Name = "Rejoin Game!",
	Content = "Need to rejoin for this to work!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end    
})

Tab2:AddButton({
	Name = "TP to Pray",
	Callback = function()
		local player = game.Players.LocalPlayer
		local workspace = game:GetService("Workspace")

		local foundPray = false

		for _, v in pairs(workspace:GetChildren()) do
			if v.Name == "Pray" then
				foundPray = true
				player.Character.HumanoidRootPart.CFrame = v.CFrame
				break
			end
		end

		if not foundPray then
			OrionLib:MakeNotification({
				Name = "Pray Fruit not found",
				Content = "Wait 30 seconds",
				Image = "rbxassetid://4483345998",
				Time = 5
			})
		end
	end
})


















